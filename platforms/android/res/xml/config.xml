<?xml version='1.0' encoding='utf-8'?>
<widget id="fr.tacticones.tact" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="RFduino">
        <param name="android-package" value="com.megster.cordova.rfduino.RFduinoPlugin" />
    </feature>
    <feature name="SmsPlugin">
        <param name="android-package" value="info.asankan.phonegap.smsplugin.SmsPlugin" />
    </feature>
    <name>Tact</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
</widget>
